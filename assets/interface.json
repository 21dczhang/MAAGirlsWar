{
    // 本文件主要定义了通用 UI 的行为和显示内容
    // 可参考文档和示例添加更多您需要的功能
    // 文档：https://maafw.xyz/docs/3.3-ProjectInterfaceV2
    // 示例：https://github.com/MaaXYZ/MaaFramework/blob/main/sample/interface.json
    "interface_version": 2,
    "name": "MaaXXX",
    "description": "少女战争 MAA Girls War!",
    "license": "MIT",
    // 填写项目地址和版本号可实现自动更新功能
    "github": "https://github.com/21dczhang/MAAGirlsWar",
    // "version": "0.0.1",
    "version": "DEBUG",
    // Mirror酱为第三方分发服务，新项目默认关闭
    // 若有需要请联系 Mirror酱 开通，直接自行开启是无法使用的
    // "mirrorchyan_rid": "MaaXXX",
    // "mirrorchyan_multiplatform": true,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb",
            "display_short_side": 720 // 默认缩放分辨率的短边长度
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio",
                // 截图及控制方式，请根据项目实际情况修改
                // 文档: https://maafw.xyz/docs/2.4-ControlMethods
                "screencap": "PrintWindow", // 截图方式
                "mouse": "PostMessageWithCursorPos", // 鼠标控制方式
                "keyboard": "PostMessageWithCursorPos" // 键盘控制方式
            }
        }
    ],
    "resource": [
        {
            "name": "台服",
            "path": [
                "./resource"
            ]
        },
        // {
        //     "name": "B 服",
        //     "path": [
        //         "./resource",
        //         "./resource_bilibili"
        //     ]
        // }
    ],
    //可选的 agent 示例
    "agent": {
        "child_exec": "python",
        "child_args": [
            "./../agent/main.py"
        ]
    },
    "task": [
        {
            "name": "启动游戏",
            "entry": "StartGame_Start",
            "default_check": true
        },
        {
            "name": "宁宁助手",
            "entry": "NingNing_Start",
            "default_check": true
        },
        {
            "name": "领取VIP经验",
            "entry": "VIP_EXP_Start",
            "default_check": true
        },
        {
            "name": "领取免费券 - 角色券 & 花嫁券",
            "entry": "Daily_UP50_Start",
            "default_check": true
        },
        {
            "name": "冒险 - 日常完成",
            "entry": "MaoXian_Start",
            "default_check": true
        },
        {
            "name": "领取日常/周常奖励",
            "entry": "Daily_Task_Start",
            "default_check": true
        },
        {
            "name": "测试任务",
            "entry": "开始遍历"
        },
        {
            "name": "测试任务2",
            "entry": "Daily_Task_Daily_Task_Start_RiHuoYueJiangLi_Start"
        }
    ],
    "option": {}
}